{
  "name": "gitsunmin",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite -c vite.config.ts",
    "build": "tsc -b && vite build",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "format": "biome format --write .",
    "preview": "vite preview",
    "deploy": "gh-pages -d dist",
    "bd": "bun run build && bun run deploy",
    "pull:modules": "git submodule update --remote --merge",
    "gen:book-cover": "bun .scripts/generate-book-cover.ts",
    "test:vi": "vitest",
    "test:ui": "vitest --ui",
    "prepare": "husky"
  },
  "dependencies": {
    "react": "^19.2.3",
    "react-dom": "^19.2.3",

    "@mdx-js/react": "^3.1.1",
