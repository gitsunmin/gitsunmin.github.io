    "@testing-library/react": "^16.3.1",
    "@types/mdx": "^2.0.13",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "@vitest/snapshot": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "caniuse-lite": "^1.0.30001763",
