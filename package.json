    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "embla-carousel-react": "^8.6.0",
    "lucide-react": "^0.562.0",
    "mermaid": "^11.12.2",
    "shiki": "^3.21.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "ts-pattern": "^5.9.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@eslint/compat": "^2.0.0",
    "@eslint/eslintrc": "^3.3.3",
